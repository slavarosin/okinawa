<h1>New component_group_text</h1>

<% form_for(@component_group_text) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :lang %><br />
    <%= f.text_field :lang %>
  </p>
  <p>
    <%= f.label :text %><br />
    <%= f.text_field :text %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', component_group_texts_path %>