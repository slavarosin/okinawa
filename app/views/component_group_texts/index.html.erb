<h1>Listing component_group_texts</h1>

<table>
  <tr>
    <th>Lang</th>
    <th>Text</th>
  </tr>

<% @component_group_texts.each do |component_group_text| %>
  <tr>
    <td><%=h component_group_text.lang %></td>
    <td><%=h component_group_text.text %></td>
    <td><%= link_to 'Show', component_group_text %></td>
    <td><%= link_to 'Edit', edit_component_group_text_path(component_group_text) %></td>
    <td><%= link_to 'Destroy', component_group_text, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New component_group_text', new_component_group_text_path %>